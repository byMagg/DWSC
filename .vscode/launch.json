{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch actividad-1",
            "type": "cppdbg",
            "request": "launch",
            "program": "/${workspaceFolder}/actividad-1/TraderConfigs",
            "args": [
                "/${workspaceFolder}/actividad-1/ejemplos/cb.txt",
                "/${workspaceFolder}/actividad-1/ejemplos/architecture.txt"
            ],
            "stopAtEntry": false,
            "cwd": "/${workspaceFolder}/actividad-1/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: g++ build active file"
        }
    ]
}